{
  "version": 2,
  "builds": [
    {
      "src": "football_calendar_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "football_calendar_backend/wsgi.py"
    }
  ]
}
